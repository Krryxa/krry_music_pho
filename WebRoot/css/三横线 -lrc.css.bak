.u-icon-detail
{
    width: 12px;
    height: 10px;
    padding: 19px 10px;
}

.u-icon-detail i
{
    display: block;
    position: relative;
    width: 12px;
    height: 2px;
    background-color: #ffa124;
}

.u-icon-detail i:before
{
    position: absolute;
    top: 4px;
    width: 12px;
    height: 2px;
    background-color: #ffa124;
    content: '';
}

.u-icon-detail i:after
{
    position: absolute;
    top: 8px;
    width: 7px;
    height: 2px;
    background-color: #ffa124;
    content: '';
}